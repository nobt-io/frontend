{
  "name": "Frontend",
  "version": "0.0.1",
  "description": "Frontend for nobt.io",
  "private": true,
  "engines": {
    "node": ">=8.5.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "dev": "better-npm-run dev",
    "lint": "babel-node node_modules/eslint/bin/eslint src && travis-lint .travis.yml",
    "test": "better-npm-run test",
    "deploy": "npm run clean && better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "webpack --mode development",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "webpack-dev-server --mode development",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "webpack --mode production",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nobt-io/frontend.git"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "fs-extra": "^0.30.0",
    "history": "~3.0.0",
    "immutability-helper": "^2.0.0",
    "ip": "^1.1.2",
    "normalize.css": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-update": "^15.3.1",
    "react-bootstrap": "^0.31.0",
    "react-copy-to-clipboard": "^4.2.3",
    "react-css-themr": "^2.1.2",
    "react-dom": "^15.0.0",
    "react-hammerjs": "^0.5.0",
    "react-headroom": "^2.1.3",
    "react-intl": "^2.1.5",
    "react-overlays": "^0.8.3",
    "react-redux": "^5.0.0",
    "react-responsive-modal": "^1.2.3",
    "react-rotating-text": "^1.2.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.0",
    "react-scrollspy": "^3.1.1",
    "react-toolbox": "^1.1.2",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.0.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.0",
    "reselect": "^2.5.4",
    "reselect-change-memoize": "^1.1.0",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "0.0.10",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.1",
    "mini-css-extract-plugin": "^0.3.0",
    "node-sass": "^4.8.3",
    "react-test-renderer": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "travis-lint": "^1.0.0",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}

{
  "name": "Frontend",
  "version": "0.0.1",
  "description": "Frontend for nobt.io",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "dev": "better-npm-run dev",
    "lint": "babel-node node_modules/eslint/bin/eslint src && travis-lint .travis.yml",
    "test": "better-npm-run test",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:nobt-io/frontend.git"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.scss$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.9.0",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "fs-extra": "^0.30.0",
    "history": "~3.0.0",
    "immutability-helper": "^2.0.0",
    "ip": "^1.1.2",
    "normalize.css": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-update": "^15.3.1",
    "react-bootstrap": "^0.31.0",
    "react-copy-to-clipboard": "^4.2.3",
    "react-css-themr": "^2.1.2",
    "react-dom": "^15.0.0",
    "react-hammerjs": "^0.5.0",
    "react-headroom": "^2.1.3",
    "react-intl": "^2.1.5",
    "react-overlays": "^0.8.3",
    "react-redux": "^5.0.0",
    "react-responsive-modal": "^1.2.3",
    "react-rotating-text": "^1.2.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.0",
    "react-scrollspy": "^3.1.1",
    "react-toolbox": "^1.1.2",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.0.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.0",
    "reselect": "^2.5.4",
    "reselect-change-memoize": "^1.1.0",
    "yargs": "^6.0.0"
  },
  "devDependencies": {
    "better-npm-run": "0.0.10",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "travis-lint": "^1.0.0",

    "identity-obj-proxy": "^3.0.0",
    "react-test-renderer": "^15.3.2",
    "jest": "^20.0.1",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.1"
  }
}
